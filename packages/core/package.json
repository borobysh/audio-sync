{
  "name": "@borobysh/audio-sync-core",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./internals": {
      "import": "./dist/internals/index.mjs",
      "require": "./dist/internals/index.js",
      "types": "./dist/internals/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/internals/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/internals/index.ts --format cjs,esm --watch --dts",
    "test": "vitest run"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.2"
  }
}
